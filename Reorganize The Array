// User Template For C++

class Solution {
  public:
    vector<int> rearrange(const vector<int>& arr) {
        int n = arr.size();
        vector<int>util(n,0);
        for(int i:arr){
            if(i<n && i>=0)util[i]=1;
        }
        vector<int>ans;
        for(int i=0;i<n;i++){
                if(util[i]>0)ans.push_back(i);
                else ans.push_back(-1);
        }
        return ans ;
    }
};
