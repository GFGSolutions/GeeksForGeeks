// User Template For C++

class Solution {
  public:
    string findLargest(vector<int> &arr) {
        sort(arr.begin(), arr.end(),[](int&a, int& b){
            string x = to_string(a); 
            string y = to_string(b); 
            return x + y > y + x;
        });
        if (!arr.empty() && arr[0] == 0) return "0";
        string ans ;
        for(auto i : arr){
            ans += to_string(i);
        }
        return ans;
    }
};
