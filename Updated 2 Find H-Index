// User Template For C++

class Solution {
  public:
    int hIndex(vector<int>& citation) {
        sort(citation.rbegin(),citation.rend());
       int n=citation.size();
       int i=1;
       for(i=1;i<=n;i++){
           if(citation[i-1]>=i)continue;
           break;
       }
       return i-1;
    }
};
