// User Template For C++

class Solution {
  public:
    int kthLargest(vector<int> &arr, int k) {
        int n = arr.size();
        priority_queue<int, vector<int>, greater<int>> pq;
        for(int i = 0, s = 0; i < n; i++, s = 0) {
            for(int j = i; j < n; j++) {
                s += arr[j];
                pq.push(s);
                if(pq.size() > k) pq.pop();
            }
        }
        return pq.top();
    }
};
