// User Template For C++

class Solution {
  public:
    void pushZerosToEnd(vector<int>& arr) {
        int cnd=0; 
        for(int i=0;i<arr.size();i++)
        {
            if(arr[i])
            {
                swap(arr[i],arr[cnd]);
                cnd++;
            }
            
        }
    }
};
