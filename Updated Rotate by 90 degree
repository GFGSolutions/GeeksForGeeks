// User Template For Java

class Solution {
  public:
    void rotateby90(vector<vector<int>>& mat) {
        int n=mat.size();
        for(int i=0;i<n;i++)
        reverse(mat[i].begin(),mat[i].end());
        for(int i=0;i<n;i++){
            for(int j=0;j<=i;j++){
                swap(mat[i][j],mat[j][i]);
            }
        }
    }
};
