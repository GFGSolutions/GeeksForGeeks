// User Template For Java

class Solution {
    public boolean isValid(String str) {
        String[] s = str.split("\\.");
        for(int i=0;i<s.length;i++){
            int x=Integer.parseInt(s[i]);
            if(x>=0 && x<=255)
                continue;
            else
                return false;
        }
        return true;
    }
}
