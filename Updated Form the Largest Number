// User Template For C++

class Solution {
  public:
    static bool compare(int a, int b){
        string s1 = to_string(a);
        string s2 = to_string(b);
        return s1 + s2 > s2 + s1;
    }
    string findLargest(vector<int> &arr) {
        sort(arr.begin(), arr.end(), compare);
        string ans = "";
        for(auto &x : arr){
            ans += to_string(x);
        }
        return ans[0] == '0' ? "0" : ans;
    }
};
